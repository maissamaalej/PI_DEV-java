<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<DialogPane headerText="Modifier Planning" prefHeight="266.0" prefWidth="331.0" translateX="1.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.popupModifierPlanController">
    <header>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #f58400;" text="Modifier Planning" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Text>
    </header>
    <content>
        <AnchorPane prefHeight="370.0" prefWidth="489.0">
            <children>
                <Label layoutX="35.0" layoutY="40.0" text="Titre">
               <font>
                  <Font size="13.0" />
               </font></Label>
                <TextField fx:id="titreModif" layoutX="91.0" layoutY="37.0" promptText="saisir titre" />
                <Label layoutX="35.0" layoutY="91.0" text="Tarif">
               <font>
                  <Font size="13.0" />
               </font></Label>
                <Button layoutX="88.0" layoutY="172.0" mnemonicParsing="false" onAction="#ModifierPlanning" style="-fx-background-color: #f58400;" text="Modifier" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button layoutX="199.0" layoutY="172.0" mnemonicParsing="false" onAction="#annulermodification" style="-fx-background-color: #f58400;" text="Annuler" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
            <TextField fx:id="tarifModif" layoutX="91.0" layoutY="88.0" promptText="saisir Tarif" />
            </children>
        </AnchorPane>
    </content>
</DialogPane>
